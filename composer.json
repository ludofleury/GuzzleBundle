{
    "name": "playbloom/guzzle-bundle",
    "description": "Provide Symfony2 web profiler for Guzzle",
    "keywords": ["guzzle", "Symfony2", "profiler"],
    "homepage": "https://github.com/ludofleury/GuzzleBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Ludovic Fleury",
            "email": "ludo.fleury@gmail.com"
        }
    ],
    "scripts": {
        "phpunit": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/phpcs --standard=phpcs.xml.dist src/ tests/",
        "phpcs-fix": "vendor/bin/phpcbf --standard=phpcs.xml.dist src/ tests/"
    },
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "~4.0|~5.0|~6.0|~7.0",
        "symfony/web-profiler-bundle": "~2.8|~3.0|^4.0",
        "symfony/http-foundation": "~2.8|~3.0|^4.0",
        "symfony/http-kernel": "~2.8|~3.0|^4.0",
        "symfony/config": "~2.8|~3.0|^4.0",
        "symfony/dependency-injection": "~2.8|~3.0|^4.0",
        "symfony/yaml": "~2.8|~3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0|^10.0|^11.0",
        "squizlabs/php_codesniffer": "^3.12",
        "symfony/stopwatch": "~2.8|~3.0|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Playbloom\\Bundle\\GuzzleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Playbloom\\Bundle\\GuzzleBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
